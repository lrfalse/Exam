<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="vo.SignupEntity" table="signup" schema="exam" dynamic-update="true" dynamic-insert="true">
        <id name="signUpId" column="signUp_id"/>
        <property name="stuId" column="stu_id"/>
        <property name="examId" column="exam_id"/>
        <property name="signUpStartTime" column="signUp_startTime"/>
        <property name="signUpEndTime" column="signUp_endTime"/>
    </class>
</hibernate-mapping>