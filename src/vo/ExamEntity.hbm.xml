<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="vo.ExamEntity" table="exam" schema="exam" dynamic-update="true" dynamic-insert="true">
        <id name="examId" column="exam_id"/>
        <property name="examName" column="exam_name"/>
        <property name="categoryId" column="category_id"/>
        <property name="examTime" column="exam_time"/>
        <property name="examPlace" column="exam_place"/>
    </class>
</hibernate-mapping>